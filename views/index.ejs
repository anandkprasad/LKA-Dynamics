    <script>
        // Smooth-scroll + focus the email field when user clicks Join the waitlist
        (function () {
            document.addEventListener('DOMContentLoaded', function () {
                var btn = document.getElementById('join-waitlist');
                if (!btn) return;
                btn.addEventListener('click', function (e) {
                    e.preventDefault();
                    var target = document.getElementById('mce-EMAIL');
                    if (target) {
                        try {
                            target.scrollIntoView({ behavior: 'smooth', block: 'center' });
                        } catch (err) {
                            // fallback
                            window.location.hash = '#contact';
                        }
                        // focus after scroll
                        setTimeout(function () {
                            try { target.focus({ preventScroll: true }); } catch (err) { target.focus(); }
                        }, 600);
                    } else {
                        // fallback to contact section if input missing
                        var contact = document.getElementById('contact');
                        if (contact) contact.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    }
                });
            });
        })();
    </script>

