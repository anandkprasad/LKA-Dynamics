                                <button id="waitlist-submit" type="submit" class="btn btn--primary">Get
                                    Notified</button>
                            </form>



                            <div class="waitlist-meta">
                                <div id="waitlist-avatars" class="waitlist-avatars" aria-hidden="true">
                                    <!-- avatars injected by JS -->
                                </div>
                            </div>


                            <div id="waitlist-status" class="waitlist-status" role="status" aria-live="polite"></div>



                            <div id="" class="">200+ people on the waitlist</div>
                        </div>

                    </div> <!-- s-intro__content-inner -->



                </div> <!-- s-intro__content -->


                <div class="s-intro__scroll">
                    <a href="#about" class="smoothscroll">
                        <span>Scroll Down</span>
